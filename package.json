{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --concurrency 11  --",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "diff:main": "turbo run build --dry-run=json --filter=[main] > dry-run.json"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.18",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
